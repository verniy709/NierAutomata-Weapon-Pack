<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
<!--== CruelArrogance ===-->
<ThingDef ParentName="NA_W_WeaponSharpBase">
  <defName>NA_W_CruelArrogance_Prototype</defName> 
  <label>CruelArrogance Prototype</label>
  <description>CruelArrogance Prototype</description>
  <graphicData>
    <texPath>Weapon/CruelArrogance</texPath>
    <graphicClass>Graphic_Single</graphicClass>
  </graphicData>
  <techLevel>Spacer</techLevel>
  <statBases>
    <WorkToMake>15000</WorkToMake>
    <Mass>2.5</Mass>
    <AccuracyTouch>0.85</AccuracyTouch>
    <AccuracyShort>0.7</AccuracyShort>
    <AccuracyMedium>0.6</AccuracyMedium>
    <AccuracyLong>0.55</AccuracyLong>
    <RangedWeapon_Cooldown>0.30</RangedWeapon_Cooldown>
  </statBases>
  <stuffCategories>
    <li>Metallic</li>
  </stuffCategories>
  <costStuffCount>150</costStuffCount>
  <costList>
  	<ComponentSpacer>5</ComponentSpacer>
  </costList>
  <weaponTags>
    <li>UltratechMelee</li>
  </weaponTags>
  <comps>
    <li Class="MVCF.Comps.CompProperties_VerbProps">
      <compClass>MVCF.Comps.Comp_VerbProps</compClass>
      <ConsiderMelee>true</ConsiderMelee>
      <verbProps>
        <li>
          <label>shockwave</label>
          <canBeToggled>true</canBeToggled>
        </li>
      </verbProps>
    </li>
  </comps>
  <verbs>
    <li>
      <verbClass>Verb_Shoot</verbClass>
      <label>shockwave</label>
      <hasStandardCommand>true</hasStandardCommand>
      <defaultProjectile>NA_W_Projectile_Cruel_M</defaultProjectile>
      <warmupTime>0.3</warmupTime>
      <range>10</range>
      <soundCast>NA_W_SwordSlide</soundCast>
      <burstShotCount>1</burstShotCount>
    </li>
  </verbs>
  <tools>
    <li>
      <label>handle</label>
      <capacities>
        <li>Blunt</li>
        <li>Poke</li>
      </capacities>
      <power>10</power>
      <cooldownTime>1.7</cooldownTime>
    </li>
    <li>
      <label>point</label>
      <capacities>
        <li>Stab</li>
      </capacities>
      <power>19</power>
      <cooldownTime>1.4</cooldownTime>
    </li>
    <li>
      <label>edge</label>
      <capacities>
        <li>Cut</li>
      </capacities>
      <power>20</power>
      <cooldownTime>1.4</cooldownTime>
    </li>
  </tools>
</ThingDef>  

