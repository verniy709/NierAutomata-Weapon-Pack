<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--== CruelLament ===-->
<ThingDef ParentName="NA_W_WeaponBluntBase">
  <defName>NA_W_CruelLament_Prototype</defName> 
  <label>CruelLament Prototype</label>
  <description>CruelLament Prototype</description>
  <graphicData>
    <texPath>Weapon/CruelLament</texPath>
    <graphicClass>Graphic_Single</graphicClass>
  </graphicData>
  <techLevel>Spacer</techLevel>
  <statBases>
    <WorkToMake>15000</WorkToMake>
    <Mass>2</Mass>
    <AccuracyTouch>0.85</AccuracyTouch>
    <AccuracyShort>0.7</AccuracyShort>
    <AccuracyMedium>0.6</AccuracyMedium>
    <AccuracyLong>0.55</AccuracyLong>
    <RangedWeapon_Cooldown>0.45</RangedWeapon_Cooldown>
  </statBases>
  <stuffCategories>
    <li>Metallic</li>
  </stuffCategories>
  <costStuffCount>150</costStuffCount>
  <costList>
    <ComponentSpacer>5</ComponentSpacer>
  </costList>
  <weaponTags>
    <li>UltratechMelee</li>
  </weaponTags>
  <comps>
    <li Class="MVCF.Comps.CompProperties_VerbProps">
      <compClass>MVCF.Comps.Comp_VerbProps</compClass>
      <verbProps>
        <li>
          <label>shockwave</label>
          <canBeToggled>true</canBeToggled>
        </li>
      </verbProps>
    </li>
  </comps>
  <verbs>
    <li>
      <verbClass>Verb_Shoot</verbClass>
      <label>shockwave</label>
      <hasStandardCommand>true</hasStandardCommand>
      <defaultProjectile>NA_W_Projectile_Cruel_S</defaultProjectile>
      <warmupTime>0.15</warmupTime>
      <range>6</range>
      <soundCast>NA_W_SwordSlide</soundCast>
      <burstShotCount>1</burstShotCount>
      <targetParams>
          <canTargetLocations>true</canTargetLocations>
      </targetParams>
    </li>
  </verbs>
  <tools>
    <li>
      <label>head</label>
      <labelUsedInLogging>false</labelUsedInLogging>
      <capacities>
        <li>Blunt</li>
      </capacities>
      <power>20</power>
      <cooldownTime>1.2</cooldownTime>
    </li>
  </tools>
</ThingDef>   

