<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--== CruelBloodOath ===-->
<ThingDef ParentName="NA_W_WeaponSharpBase">
  <defName>NA_W_CruelBloodOath_Prototype</defName> 
  <label>CruelBloodOath Prototype</label>
  <description>CruelBloodOath Prototype</description>
  <graphicData>
    <texPath>Weapon/CruelBloodOath</texPath>
    <graphicClass>Graphic_Single</graphicClass>
  </graphicData>
  <techLevel>Spacer</techLevel>
  <statBases>
    <WorkToMake>15000</WorkToMake>
    <Mass>3</Mass>
    <AccuracyTouch>0.85</AccuracyTouch>
    <AccuracyShort>0.7</AccuracyShort>
    <AccuracyMedium>0.6</AccuracyMedium>
    <AccuracyLong>0.55</AccuracyLong>
    <RangedWeapon_Cooldown>0.60</RangedWeapon_Cooldown>
  </statBases>
  <stuffCategories>
    <li>Metallic</li>
  </stuffCategories>
  <costStuffCount>180</costStuffCount>
  <costList>
    <ComponentSpacer>5</ComponentSpacer>
  </costList>
  <weaponTags>
    <li>UltratechMelee</li>
  </weaponTags>
  <comps>
    <li Class="MVCF.Comps.CompProperties_VerbProps">
      <compClass>MVCF.Comps.Comp_VerbProps</compClass>
      <verbProps>
        <li>
          <label>shockwave</label>
          <canBeToggled>true</canBeToggled>
        </li>
      </verbProps>
    </li>
  </comps>
  <verbs>
    <li>
      <verbClass>Verb_Shoot</verbClass>
      <label>shockwave</label>
      <hasStandardCommand>true</hasStandardCommand>
      <defaultProjectile>NA_W_Projectile_Cruel_L</defaultProjectile>
      <warmupTime>0.35</warmupTime>
      <range>14</range>
      <soundCast>NA_W_SwordSlide</soundCast>
      <burstShotCount>1</burstShotCount>
      <targetParams>
          <canTargetLocations>true</canTargetLocations>
      </targetParams>
    </li>
  </verbs>
  <tools>
    <li>
      <label>handle</label>
      <capacities>
        <li>Blunt</li>
      </capacities>
      <power>12</power>
      <cooldownTime>2</cooldownTime>
    </li>
    <li>
      <label>point</label>
      <capacities>
        <li>Stab</li>
      </capacities>
      <power>24</power>
      <cooldownTime>1.8</cooldownTime>
    </li>
    <li>
      <label>edge</label>
      <capacities>
        <li>Cut</li>
      </capacities>
      <power>26</power>
      <cooldownTime>1.8</cooldownTime>
    </li>
  </tools>
</ThingDef>   

