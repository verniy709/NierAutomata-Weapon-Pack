<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--== CruelOath ===-->
<ThingDef ParentName="NA_W_WeaponSharpBase">
  <defName>NA_W_CruelOath_Prototype</defName> 
  <label>CruelOath Prototype</label>
  <description>CruelOath Prototype</description>
  <graphicData>
    <texPath>Weapon/CruelOath</texPath>
    <graphicClass>Graphic_Single</graphicClass>
  </graphicData>
  <techLevel>Spacer</techLevel>
  <statBases>
    <WorkToMake>15000</WorkToMake>
    <Mass>1</Mass>
    <AccuracyTouch>0.8</AccuracyTouch>
    <AccuracyShort>0.65</AccuracyShort>
    <AccuracyMedium>0.45</AccuracyMedium>
    <AccuracyLong>0.30</AccuracyLong>
    <RangedWeapon_Cooldown>0.45</RangedWeapon_Cooldown>
  </statBases>
  <stuffCategories>
    <li>Metallic</li>
  </stuffCategories>
  <costStuffCount>120</costStuffCount>
  <costList>
    <ComponentSpacer>5</ComponentSpacer>
  </costList>
  <weaponTags>
    <li>UltratechMelee</li>
  </weaponTags>
  <comps>
		<li Class="MVCF.Comps.CompProperties_VerbProps">
      <compClass>MVCF.Comps.Comp_VerbProps</compClass>
      <verbProps>
        <li>
          <label>shockwave</label>
          <canBeToggled>true</canBeToggled>
        </li>
      </verbProps>
    </li>
  </comps>
  <verbs>
    <li>
      <verbClass>Verb_Shoot</verbClass>
      <label>shockwave</label>
      <hasStandardCommand>true</hasStandardCommand>
      <defaultProjectile>NA_W_Projectile_Cruel_S</defaultProjectile>
      <warmupTime>0.2</warmupTime>
      <range>8</range>
      <soundCast>NA_W_SwordSlide</soundCast>
      <burstShotCount>1</burstShotCount>
      <targetParams>
          <canTargetLocations>true</canTargetLocations>
      </targetParams>
    </li>
  </verbs>
  <tools>
    <li>
      <label>handle</label>
      <capacities>
        <li>Blunt</li>
      </capacities>
      <power>9</power>
      <cooldownTime>1.6</cooldownTime>
    </li>
    <li>
      <label>point</label>
      <capacities>
        <li>Stab</li>
      </capacities>
      <power>16</power>
      <cooldownTime>1.2</cooldownTime>
    </li>
    <li>
      <label>edge</label>
      <capacities>
        <li>Cut</li>
      </capacities>
      <power>18</power>
      <cooldownTime>1.2</cooldownTime>
    </li>
  </tools>
</ThingDef>   

