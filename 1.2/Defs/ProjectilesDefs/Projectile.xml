<Defs>

  <!--== Projectile ===-->

   <ThingDef ParentName="BaseGrenadeProjectile">
    <defName>BB_W_EmilHeads_Shell</defName>
    <label>EmilHeads Shell</label>
    <graphicData>
      <texPath>Projectile/EmilHeads_Shell</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
    </graphicData>
    <thingClass>Projectile_Explosive</thingClass>
    <projectile>
      <explosionRadius>3.9</explosionRadius >
      <damageDef>Bomb</damageDef>
      <speed>50</speed>
    </projectile>
  </ThingDef>
  
  <!--== Projectile Base===-->

  <VFECore.ExpandableProjectileDef Name="NA_W_Projectile_Cruel" ParentName="BaseBullet">
    <defName>NA_W_Projectile_Cruel</defName>
    <label>Cruel shockwave</label>
    <thingClass>VFECore.ExpandableProjectile</thingClass>
    <graphicData>
      <texPath>Effect/Proj_Cruel</texPath>
      <texPathFadeOut>Effect/Proj_CruelFadeOut</texPathFadeOut>
      <shaderType>Mote</shaderType>
    </graphicData>
    <drawOffscreen>true</drawOffscreen>
    <lifeTimeDuration>15</lifeTimeDuration>
    <widthScaleFactor>1</widthScaleFactor>
    <heightScaleFactor>1</heightScaleFactor>
    <startingPositionOffset>(0, 0, -1)</startingPositionOffset>
    <totalSizeScale>0.75</totalSizeScale>
    <tickFrameRate>4</tickFrameRate>
    <finalTickFrameRate>8</finalTickFrameRate>
    <tickDamageRate>5</tickDamageRate>
    <dealsDamageOnce>false</dealsDamageOnce>
    <minDistanceToAffect>1</minDistanceToAffect>
    <projectile>
      <damageDef>Cut</damageDef>    
      <flyOverhead>false</flyOverhead>
      <damageAmountBase>1</damageAmountBase>
      <stoppingPower>2.0</stoppingPower>
      <armorPenetrationBase>0.30</armorPenetrationBase>
      <speed>40</speed>
    </projectile>
  </VFECore.ExpandableProjectileDef>

